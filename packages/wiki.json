{
    "name": "wiki",
    "author": "BetaPictoris",
    "description": "View Wikipedia articles through the CLI.",
    "url": "https://github.com/BetaPictoris/wiki.git",
    "license": "MIT",
    "language": "Go",
    "bin": {
        "in_source": [
            "build/wiki"
        ],
        "installed": [
            "wiki"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "mkdir -p build",
                "make binary"
            ],
            "update": [
                "mkdir -p build",
                "make binary"
            ]
        }
    },
    "deps": {
        "all": [
            "go",
            "make"
        ]
    }
}