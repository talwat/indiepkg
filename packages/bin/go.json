{
    "name": "go",
    "author": "go",
    "description": "An open source programming language supported by Google.",
    "download": {
        "darwin": {
            "amd64": "https://dl.google.com/go/go1.18.2.darwin-amd64.tar.gz",
            "arm64": "https://dl.google.com/go/go1.18.2.darwin-arm64.tar.gz"
        },
        "linux": {
            "amd64": "https://dl.google.com/go/go1.18.2.linux-amd64.tar.gz",
            "arm64": "https://dl.google.com/go/go1.18.2.linux-arm64.tar.gz"
        }
    },
    "version": "1.18.2",
    "license": "BSD-3-Clause",
    "bin": {
        "in_source": [
            "go/bin/go",
            "go/bin/gofmt"
        ],
        "installed": [
            "go",
            "gofmt"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cp go.indiepkg.downloaded go.tar.gz",
                "rm -rf go",
                "tar -zxf go.tar.gz"
            ],
            "update": [
                "cp go.indiepkg.downloaded go.tar.gz",
                "rm -rf go",
                "tar -zxf go.tar.gz"
            ]
        }
    },
    "config_paths": [
        "go"
    ]
}