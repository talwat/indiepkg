{
    "name": "difftastic",
    "author": "Wilfred",
    "description": "A diff that understands syntax.",
    "url": "https://github.com/Wilfred/difftastic.git",
    "license": "MIT",
    "language": "Rust",
    "bin": {
        "in_source": [
            "target/release/difft"
        ],
        "installed": [
            "difft"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build -v --release"
            ],
            "update": [
                "cargo build -v --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}