{
    "name": "cmatrix",
    "author": "abishekvashok",
    "description": "Terminal based \"The Matrix\" like implementation.",
    "url": "https://github.com/abishekvashok/cmatrix.git",
    "license": "GPL-3.0",
    "bin": {
        "in_source": [
            "cmatrix"
        ],
        "installed": [
            "cmatrix"
        ]
    },
    "manpages": [
        "cmatrix.1"
    ],
    "commands": {
        "all": {
            "install": [
                "autoreconf -i",
                "./configure",
                "make"
            ],
            "update": [
                "autoreconf -i",
                "./configure",
                "make"
            ]
        },
        "darwin": {
            "install": [
                "autoreconf -i",
                "./configure",
                "gmake"
            ],
            "update": [
                "autoreconf -i",
                "./configure",
                "gmake"
            ]
        }
    },
    "deps": {
        "all": [
            "gcc",
            "autoconf",
            "autoreconf",
            "automake"
        ],
        "linux": [
            "make"
        ],
        "darwin": [
            "gmake"
        ]
    }
}