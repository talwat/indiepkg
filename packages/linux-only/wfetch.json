{
    "name": "wfetch",
    "author": "Gcat101",
    "description": "Neofetch/pfetch, but for weather",
    "url": "https://github.com/gcat101/wfetch.git",
    "license": "GPL-3.0",
    "bin": {
        "in_source": [
            "wfetch/wfetch.py"
        ],
        "installed": [
            "/usr/local/bin/wfetch"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "sudo ./install.sh"
            ],
            "uninstall": [
                "sudo rm /usr/local/bin/wfetch"
            ],
            "update": [
                "sudo ./install.sh"
            ]   
        }
    },
    "deps": {
        "all": [
            "python3",
            "python3-pip",
            "bash"
        ]
    },
    "config_paths": [
        "/opt/wfetch"
    ]
}