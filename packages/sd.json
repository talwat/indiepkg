{
    "name": "sd",
    "author": "chmln",
    "description": "Intuitive find & replace CLI (sed alternative).",
    "url": "https://github.com/chmln/sd.git",
    "license": "MIT",
    "language": "Rust",
    "bin": {
        "in_source": [
            "target/release/sd"
        ],
        "installed": [
            "sd"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build -v --release"
            ],
            "update": [
                "cargo build -v --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}