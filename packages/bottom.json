{
    "name": "bottom",
    "author": "ClementTsang",
    "description": "Yet another cross-platform graphical process/system monitor.",
    "url": "https://github.com/ClementTsang/bottom.git",
    "license": "MIT",
    "language": "Rust",
    "bin": {
        "in_source": [
            "target/release/btm"
        ],
        "installed": [
            "btm"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build -v --release"
            ],
            "update": [
                "cargo build -v --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}