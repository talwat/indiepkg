{
    "name": "bat",
    "author": "sharkdp",
    "description": "A cat(1) clone with wings.",
    "url": "https://github.com/sharkdp/bat.git",
    "license": "Apache-2.0",
    "bin": {
        "in_source": [
            "target/release/bat"
