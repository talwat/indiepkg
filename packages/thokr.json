{
    "name": "thokr",
    "author": "coloradocolby",
    "description": "A sleek typing tui written in rust.",
    "url": "https://github.com/coloradocolby/thokr.git",
    "license": "MIT",
    "language": "Rust",
    "bin": {
        "in_source": [
            "target/release/thokr"
        ],
        "installed": [
            "thokr"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build -v --release"
            ],
            "update": [
                "cargo build -v --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}