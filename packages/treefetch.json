{
    "name": "treefetch",
    "author": "angelofallars",
    "description": "A plant-based system fetch tool made with Rust.",
    "url": "https://github.com/angelofallars/treefetch.git",
    "license": "GPL-3.0",
    "bin": {
        "in_source": [
            "target/release/treefetch"
        ],
        "installed": [
            "treefetch"
        ]
    },
    "commands": {
        "all": {
            "install": [
                "cargo build -v --release"
            ],
            "update": [
                "cargo build -v --release"
            ]
        }
    },
    "deps": {
        "all": [
            "rustc",
            "cargo"
        ]
    }
}